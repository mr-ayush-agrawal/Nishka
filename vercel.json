{
    "version": 2,
    "builds": [
        {
            "src": "frontend.py",
            "use": "@vercel/python"
        },
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "frontend.py"
        },
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}